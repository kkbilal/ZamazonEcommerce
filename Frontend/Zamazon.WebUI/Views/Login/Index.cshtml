@model CreateLoginDto
@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<title> Login </title>
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<!-- Google Font -->
	<link href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;500;600&display=swap" rel="stylesheet">

	<!-- Icons -->
	<link rel="stylesheet" href="~/colorlib-regform-30fonts/line-awesome/css/line-awesome.min.css">

	<!-- CSS -->
	<link rel="stylesheet" href="~/colorlib-regform-30/css/style.css">
</head>
<body>
	<div class="page-content">
		<div class="form-v4-content">
			<div class="form-left">
				<h2>Welcome Back!</h2>
				<p><a href="/Register/Index/">Register</a> Now and continue exploring amazing features with us.</p>

			</div>

			<!-- Form -->
			<form class="form-detail" method="post">
	
				<label for="UserName">UserName</label>
				<input type="text" asp-for="UserName" id="UserName">
				<label for="password">Password</label>
				<input type="password" asp-for="Password" id="password" required>

				<button class="register">Register</button>
			</form>
		</div>
	</div>

	<!-- JS: jQuery + Validation -->
	<script src="https://code.jquery.com/jquery-1.11.1.min.js"></script>
	<script src="https://cdn.jsdelivr.net/jquery.validation/1.16.0/jquery.validate.min.js"></script>
	<script src="https://cdn.jsdelivr.net/jquery.validation/1.16.0/additional-methods.min.js"></script>

	
</body>
</html>
